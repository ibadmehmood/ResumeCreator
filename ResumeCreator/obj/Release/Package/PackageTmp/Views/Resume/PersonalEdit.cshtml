@model ResumeCreator.Models.PersonalInfo
@{
    Layout = null;
}
<!DOCTYPE html>
<head>

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"> </script>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- Compiled and minified JavaScript -->

    <link rel="stylesheet" href="~/Content/customstyles.css">
</head>
<body style="position: relative;">
    @Html.Partial("NavBar")

    

    <!--page content-->
    <div class="container" style="margin-top:30px;">
        <div id="test1" class="col s12">
            <div class="row">
                <form class="col s12" method="post" action="/Resume/Personal/Edit">
                    <div class="row">
                        <div class="col s12">
                            <span style="font-size:1.6em">EDIT YOUR</span><span style="font-size:1.6em;font-weight:bold;"> RESUME HEADING</span>
                            <p style="font-size:1.3em">Employers will use this information to contact you.</p>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s6">
                            @Html.AntiForgeryToken()
                            <input  name="resumeid" type="hidden" class="validate" value="@Model.id">
                            <input id="first_name" name="first_name" type="text" class="validate" value="@Model.first_name">
                            <label for="first_name">First Name</label>
                        </div>
                        <div class="input-field col s6">
                            <input id="last_name" name="last_name" type="text" class="validate" value="@Model.last_name">
                            <label for="last_name">Last Name</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="address" name="address" type="text" class="validate" value="@Model.address">
                            <label for="address">Address</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s4">
                            <input id="city" type="text" name="city" class="validate" value="@Model.city">
                            <label for="city">City</label>
                        </div>
                        <div class="input-field col s4">
                            <input id="state" type="text" name="state" class="validate" value="@Model.state">
                            <label for="state">State</label>
                        </div>
                        <div class="input-field col s4">
                            <input id="zip_code" type="text" name="zip_code" class="validate" value="@Model.zip_code">
                            <label for="zip_code">ZIP code</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s6">
                            <input id="email" type="email" name="email" class="validate" value="@Model.email">
                            <label for="email">Email</label>
                        </div>
                        <div class="input-field col s6">
                            <input id="phone" name="phone" type="number" class="validate" value="@Model.phone">
                            <label for="phone">Phone</label>
                        </div>
                    </div>
                    <div class="row">



                        <div class="col s4 ">

                            <a class="waves-effect waves-light btn-large" href="/Resume/Review/@Model.id">BACK</a>
                        </div>

                        <div class="col s4 offset-s4 right-align">

                            <button class="waves-effect waves-light btn-large" type="submit">UPDATE & NEXT</button>
                        </div>
                    </div>
                </form>
            </div>

        </div>
    </div>
    <!-- end of page content-->
    <div class="" style="margin-top: 30px;font-size: 1.6em;">
        <div class="container">
            <div class="row">
                <div class="col " style="color:black;">

                </div>
            </div>
        </div>

    </div>
    <div class="" style="margin-top:30px;">
        <footer class="page-footer teal darken-1" style="position: absolute;bottom: 0;left:0;right:0;">
            <div class="container ">
                <div class="row teal darken-1">
                    <div class="footer-copyright teal darken-1">
                        <div class="container teal darken-1" style="color:#ffff">
                            © QuickResume 2019
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>
</body>
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
<script>

    //var instance = M.Tabs.init(el, options);

    // Or with jQuery

    $(document).ready(function () {
        $('.tabs').tabs();
        $('.datepicker').datepicker();
        $(".dropdown-trigger").dropdown();
    });
</script>
</html>





